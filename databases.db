create DATABASE TrueP;
use TrueP;
create table if not exists User(
	id int primary key auto_increment,
    full_name varchar(255),
	username varchar(32),
	password varchar(255),
	email varchar(255),
	title varchar(255),
	image varchar(2048),
	dob varchar(255),
	phone varchar(20),
	address varchar(2048)
);